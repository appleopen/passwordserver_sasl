# Compatibility function build rules (they build in lib/)
$(COMPAT_OBJS):
	rm -f $(COMPAT_OBJS)
	cd ../lib; $(MAKE) $(COMPAT_OBJS)
	for file in $(COMPAT_OBJS); do ln -s ../lib/$$file .; done

